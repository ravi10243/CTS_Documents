/* ---------------------- Pagination ------------------------- */

.view_resource_module .button_block{
    display:block;
    border-top: 1px solid #d4ccd5;
    border-bottom: 1px solid #d4ccd5;
    margin-top: 10%;
    margin-bottom: 15%;
}
.view_resource_module .load_less{
    display:none;
}
.view_resource_module .load_more, .view_resource_module .load_less{
    margin-top: 10%;
    margin-bottom: 10%;
    padding: 8% 10%;
    width: 102%;
    color:#fff !important;
    background: black;
    border:0;
    outline:none;
    font-family: Beatrice-light;
    font-size: 22px;
}
        .showVmenu{
        display:block !important;
        }
.view_resource_module .load_more span{
    color:#fff;
}
.view_resource_module img.down_arrow {
    float: right;
        display:none;
}
.view_resource_module .pagination {
    padding:50px;
    text-align: center;
    width: 94%;
    border-top: 1px solid #d4ccd5;
    border-bottom: 1px solid #d4ccd5;
    border-radius: unset;
}
.view_resource_module .pagination,
.view_resource_module .pagination * {
  -webkit-user-select: none;
     -moz-user-select: none;
      -ms-user-select: none;
          user-select: none;
}
.view_resource_module .pagination a {
    display: inline-block;
    padding: 0 10px;
    cursor: pointer;
    color: #A7756E;
    font-family: Beatrice-light;
    font-size: 20px;
}
.view_resource_module .pagespan{
    color:#d4ccd5 !important;
}
.view_resource_module .pagination a.disabled {
    opacity: .3;
    pointer-events: none;
    cursor: not-allowed;
}    
        
