                    </aura:if>
                    <!--
                    <aura:if isTrue="{!and(v.showAGNShare, v.assessment.Share_Capture_AGN__c == 'Percentage')}">
                        <td colspan="{!v.colspan}" onclick="{!c.toggleAGNShare}" class="gps-header gps-header-agn-share">&#9668; {!$Label.c.AGN_Share_Percentage}</td>
                    </aura:if>
					-->
