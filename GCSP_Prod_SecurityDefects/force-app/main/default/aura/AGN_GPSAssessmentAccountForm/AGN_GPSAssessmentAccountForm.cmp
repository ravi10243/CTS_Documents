                        <aura:if isTrue="{!v.assessment.Consent_for_Digital_AGN__c}"><td class="gps-header" /></aura:if>
                        <aura:if isTrue="{!v.assessment.Digital_Only_AGN__c}"><td class="gps-header" /></aura:if>
