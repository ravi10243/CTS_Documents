<!-- AGN_ICL_OAMClinicListView -->
<aura:component implements="forceCommunity:availableForAllPageTypes" access="global" controller="AGN_ICL_ClinicDetailListCntrl">
    
	<ltng:require styles="{!$Resource.AGN_ICL_SLDS_270 +'/styles/salesforce-lightning-design-system.min.css'}" /> 
    <aura:attribute type="AGN_ICL_ClinicDetailListCntrl.AccountNameHasLL[]" name="paginationAccountDetails"/> 
    <aura:attribute type="String" name="UserName"/>
    <aura:attribute type="AGN_ICL_ClinicDetailListCntrl.AccountNameHasLL[]" name="Accountdetails"/>
    <aura:attribute type="Integer" name="start"/>
    <aura:attribute type="Integer" name="end"/>
    <aura:attribute type="Integer" name="previous"/>
    <aura:attribute type="boolean" name="ispreviousdisable"/>
    <aura:attribute type="boolean" name="isnextdisable"/>
    <aura:attribute name="openModal" type="boolean" default="false"/>
    
     <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <meta name="format-detection" content="telephone=no" />
    <link href="{!$Resource.AGN_ICL_OAM_CSS+'/ICL/css/bootstrap.css'}" rel="stylesheet" id="bootstrap-css"/>
    <link rel="stylesheet" href="{!$Resource.AGN_ICL_OAM_CSS+'/ICL/css/bootstrap-select.min.css'}"/>
    <link href="{!$Resource.AGN_ICL_OAM_CSS+'/ICL/css/mystyle.css'}" rel="stylesheet"/>
    <ltng:require scripts="{!$Resource.AGN_ICL_OAM_CSS + '/ICL/js/jquery11.js'}" afterScriptsLoaded="{!c.scriptsLoaded}" />
    <ltng:require scripts="{!$Resource.AGN_ICL_OAM_CSS+'/ICL/js/bootstrap.js'}"/>
    
    <div class="ICL_page_content registration_wrapper">
           
         <div class="container">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h3 style="font-weight: bold;padding-top: 8px;" class="panel-title pull-left">
                            {!$Label.c.AGN_ICL_CLINIC_DETAILS_LIST}
                        </h3>
                        
                        <!--<a href="#">--><!--<button class="btn btn-default pull-right" onclick="{!c.NewClinic}">{!$Label.c.AGN_ICL_Add_New_Clinic}</button>--><!--</a>-->
                        <div class="clearfix"></div>
                    </div>
                    <div style="margin-bottom: 50px;background-color: #fff;" class="table-responsive panel-body">
                        
                        <table id="clinic_list" class="slds-table slds-table--bordered slds-table--striped">
                            <thead>
                                <tr>
                                    <th scope="col"><span class="slds-truncate">{!$Label.c.AGN_ICL_OAMClinicListViewName}</span></th>
                                    <th scope="col"><span class="slds-truncate">{!$Label.c.AGN_ICL_OAMClinicListViewLLAvailable}</span></th>
                                    <th scope="col"><span class="slds-truncate">{!$Label.c.AGN_ICL_Delete}</span></th>
                                </tr>
                            </thead>
                             <tbody>
                                <aura:iteration items="{!v.paginationAccountDetails}" var="clinic" indexVar ="index">
                                    <tr>
                                        <td><lightning:button value="{!clinic}"  onclick="{!c.RegistrationOrDetailpage}" >{!clinic.Accname}</lightning:button></td>
                                        <td>
                                            <aura:if isTrue="{!clinic.IsLocatorListingActive}">
                                                <img style="width: 20px;" src="{!$Resource.AGN_ICL_OAM_CSS+'/ICL/images/tik.png'}"/>
                                                <aura:set attribute="else">
                                                    <img style="width: 20px;" src="{!$Resource.AGN_ICL_OAM_CSS+'/ICL/images/cross.png'}"/>
                                                </aura:set>
                                            </aura:if>
                                        </td>
                                        <td>
                                            <aura:if isTrue="{!clinic.IsSoldTo}">
                                                <p data-placement="top" data-toggle="tooltip" title="{!$Label.c.AGN_ICL_Delete}">
                                                    <lightning:button aura:id="btnId" value="{!index}"  disabled="{!clinic.IsSoldTo}" variant="brand" class="btn btn-danger btn-xs" onclick="{! c.deleteLocatorListing}">
                                                        <span class="glyphicon glyphicon-trash"></span>
                                                    </lightning:button>
                                                </p>
                                                <aura:set attribute="else">
                                                    <p data-placement="top" data-toggle="tooltip" title="{!$Label.c.AGN_ICL_Delete}">
                                                        <lightning:button aura:id="btnId" value="{!clinic.LocatorListingId}"  disabled="{!!clinic.IsLocatorListingActive}" variant="brand" class="btn btn-danger btn-xs" onclick="{! c.deleteLocatorListing}">
                                                            <span class="glyphicon glyphicon-trash"></span>
                                                        </lightning:button>
                                                    </p>
                                                </aura:set>
                                            </aura:if>
                                        </td>
                                    </tr>
                                 </aura:iteration>
                            </tbody>
                        </table>
                        <lightning:button class="page_prev" disabled="{!v.ispreviousdisable}" onclick="{!c.PreviousPage}" >{!$Label.c.AGN_ICL_Previous}</lightning:button>
                        <lightning:button class="page_next"  disabled="{!v.isnextdisable}" onclick="{!c.NextPage}" >{!$Label.c.AGN_ICL_Next}</lightning:button>
                    </div>
             </div>
        </div>
        
    </div>
    
 
</aura:component>